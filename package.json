{
  "name": "bunnyshell-hack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --pretty --noEmit",
    "prebuild": "rimraf dist",
    "prepare": "is-ci || husky install",
    "auth": "nx run auth:serve",
    "auth:show": "(cd apps/auth/ && npx prisma studio)",
    "auth:dev": "(cd apps/auth/ && npx prisma migrate dev)",
    "auth:push": "(cd apps/auth/ && npx prisma db push --accept-data-loss)",
    "auth:gen": "(cd apps/auth/ && npx prisma generate)",
    "auth:reset": "(cd apps/auth/ && npx prisma migrate reset && npx prisma generate)"
  },
  "private": true,
  "dependencies": {
    "@apollo/gateway": "^2.4.2",
    "@apollo/server": "^4.7.0",
    "@apollo/subgraph": "^2.4.2",
    "@emotion/react": "11.10.8",
    "@emotion/server": "11.10.0",
    "@emotion/styled": "11.10.8",
    "@nestjs/apollo": "^11.0.5",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/graphql": "^11.0.5",
    "@nestjs/platform-express": "^9.4.0",
    "@prisma/client": "^4.13.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.21.3",
    "next": "13.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "11.10.8",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.4.0",
    "@nx/cypress": "16.0.1",
    "@nx/eslint-plugin": "16.0.1",
    "@nx/jest": "16.0.1",
    "@nx/js": "16.0.1",
    "@nx/linter": "16.0.1",
    "@nx/nest": "16.0.1",
    "@nx/next": "^16.0.1",
    "@nx/node": "16.0.1",
    "@nx/react": "16.0.1",
    "@nx/webpack": "16.0.1",
    "@nx/workspace": "16.0.1",
    "@testing-library/react": "14.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-jest": "^29.4.1",
    "commitizen": "^4.3.0",
    "cypress": "^12.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "~8.39.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "graphql-type-json": "^0.3.2",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nx": "16.0.1",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "prisma-graphql-type-decimal": "^3.0.0",
    "prisma-nestjs-graphql": "^18.0.0",
    "react-test-renderer": "18.2.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~4.9.5"
  },
  "lint-staged": {
    "*": [
      "nx run-many --all --target=lint",
      "nx format:write --uncommitted"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
